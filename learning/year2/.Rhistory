data$Year <- year(data$Board.Approval.Date)
##Aggregating the total disbursements with the respective ie grouping the year and respective sums disbursed
d_yearly <- data %>%
group_by(Approval_Year) %>% #group the data by the year column
summarise(Total_Disbursement = sum(Disbursed.Amount..US.., na.rm = TRUE))%>%  #sums of the amount without na values
filter(!is.na(Year)) #the years should not be na
